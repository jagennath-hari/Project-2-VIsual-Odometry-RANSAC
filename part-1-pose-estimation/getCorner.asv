                            [(0.152*rem(i,84) + 0.152*rem(i,84) + 0.152) (0.152*12 +(0.178*2))];
                            [(0.152*rem(i,84) + 0.152*rem(i,84) + 0.152) (0.152*12 +(0.178*2) + 0.152)];
                            [(0.152*rem(i,84) + 0.152*rem(i,84)) (0.152*12+(0.178*2) + 0.152)];
                            [(0.152*rem(i,84) + 0.152*rem(i,84)) (0.152*12+(0.178*2))]];
        case {96, 97, 98, 99, 100, 101, 102, 103, 104, 105, 106, 107}
            m(string(i)) = [[(0.152*rem(i,96) + 0.152*rem(i,96) + 0.152/2) (0.152*14 + (0.178*2) + 0.152/2)];
                            [(0.152*rem(i,96) + 0.152*rem(i,96) + 0.152) (0.152*14 +(0.178*2))];
                            [(0.152*rem(i,96) + 0.152*rem(i,96) + 0.152) (0.152*14 +(0.178*2) + 0.152)];
                            [(0.152*rem(i,96) + 0.152*rem(i,96)) (0.152*14+(0.178*2) + 0.152)];
                            [(0.152*rem(i,96) + 0.152*rem(i,96)) (0.152*14+(0.178*2))]];
    end
end

