                            [(0.152*rem(i,60) + 0.152*rem(i,60)) (0.152*9+0.178 + 0.152)];
                            [(0.152*rem(i,60) + 0.152*rem(i,60)) (0.152*9+0.178)]];
        case {72, 73, 74, 75, 76, 77, 78, 79, 80, 81, 82, 83}
            m(string(i)) = [[(0.152*rem(i,72) + 0.152*rem(i,72) + 0.152/2) (0.152*10 + (0.178*2) + 0.152/2)];
                            [(0.152*rem(i,72) + 0.152*rem(i,72) + 0.152) (0.152*10 +(0.178*2))];
                            [(0.152*rem(i,72) + 0.152*rem(i,72) + 0.152) (0.152*10 +(0.178*2) + 0.152)];
